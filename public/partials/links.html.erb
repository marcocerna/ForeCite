<div>
  <div id="left">
    <h3 id="domains-header">Domains</h3>
    <ul>
      <a class="domain" href="http://{{currentDomain}}" target="_blank">
        <li class="list-element" ng-repeat="domain in domains" ng-mouseover="$parent.currentDomain = domain">
          {{domain}}
        </li>
      </a>
    </ul>
  </div>

  <div id="right">
    <h3 id="domains-header">Current Domain is {{currentDomain}}</h3>
    <ul>
      <li class="list-element right-list-element" ng-repeat="link in links | filter:currentDomain">
        <a class="white-text" href="{{link['*']}}" target="_blank">{{link["*"] }}</a>
      </li>
    </ul>
  </div>
</div>